<h1>Welcome to the show page for <%= @dino.name %></h1>

<label>Name: </label>
<%= @dino.name %><br />
<label>Weight: </label>
<%= @dino.weight %><br />
<label>Badass Index: </label>
<%= @dino.badass_index %><br />
<label>Category: </label>
<%= link_to @dino.dinosaur_type.name, dinosaur_type_path(@dino.dinosaur_type) %>
<label>Prey: </label>
<% @dino.preys.each do |prey| %>
<%= link_to prey.name, prey_path(prey) %><br /><br />
<% end %>

<%= link_to "Edit", edit_dinosaur_path(@dino) %>
